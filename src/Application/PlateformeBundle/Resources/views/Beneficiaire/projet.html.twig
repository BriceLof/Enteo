<tr>
    <td>Date pr√©vue d'Accompagnement</td>
    <td style="padding-left: 13px">
        {% if beneficiaire.accompagnement.dateDebut is not null %}
            Du {{ dateCourtFr(beneficiaire.accompagnement.dateDebut) }} <br>
        {% endif %}
        {% if beneficiaire.accompagnement.dateFin is not null %}
            Au {{ dateCourtFr(beneficiaire.accompagnement.dateFin) }}
        {% endif %}
    </td>
</tr>
{% if beneficiaire.accompagnement.heure is not null %}
    <tr>
        <td> Accompagnement en Heures :</td>
        <td class="projet"> {{ beneficiaire.accompagnement.heure }} h </td>
    </tr>
{% endif %}
